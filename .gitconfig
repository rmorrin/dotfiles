[user]
	name = Ryan Morrin
	email = ryanmorrin@gmail.com
[core]
	autocrlf = true
	editor = vim
	whitespace = trailing-space,space-before-tab
	filemode = false
[apply]
	whitespace = fix
[http]
	postBuffer = 524288000
[merge]
	tool = kdiff3
[mergetool "kdiff3"]
	path = C:/Program Files (x86)/KDiff3/kdiff3.exe
[mergetool "vsdiffmerge"]
	cmd = \"C:\\Program Files (x86)\\Microsoft Visual Studio 12.0\\Common7\\IDE\\vsdiffmerge.exe\" \"$REMOTE\" \"$LOCAL\" \"$BASE\" \"$MERGED\" //m
	keepbackup = false
	trustexistcode = true
[diff]
	guitool = kdiff3
[difftool "kdiff3"]
	path = C:/Program Files (x86)/KDiff3/kdiff3.exe
	cmd = \"C:/Program Files (x86)/KDiff3/kdiff3.exe\" \"$LOCAL\" \"$REMOTE\"
[difftool "vsdiffmerge"]
	cmd = \"C:\\Program Files (x86)\\Microsoft Visual Studio 12.0\\Common7\\IDE\\vsdiffmerge.exe\" \"$LOCAL\" \"$REMOTE\" //t
	keepbackup = false
	trustexistcode = true
[push]
	default = simple
[color "status"]
	changed = magenta normal bold
	untracked = magenta normal bold
[alias]
	lg = log --color --graph --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr)%C(bold blue)<%an>%Creset' --abbrev-commit
	pushall = !git remote | xargs -l git push
	last = log -1 --stat
	unstage = reset HEAD --
[commit]
	template = C:/Users/Ryan/.gitmessage.txt
